l2 = 1			# contrast less
r1 = 4			# bright more
r2 = 2			# contrast more
right_analog_up = *			# volume increase
right_analog_up = add_shift
right_analog_down = /		# volume decrease
right_analog_left = [		# speed down 10%
right_analog_right = ]		# speed up 10%
